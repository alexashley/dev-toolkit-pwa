{
  "name": "dev-toolkit-pwa",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "highlight.js": "9.14.2",
    "react": "16.8.0-alpha.1",
    "react-dom": "16.8.0-alpha.1",
    "react-icons": "3.3.0",
    "react-scripts": "2.1.3"
  },
  "scripts": {
    "start": "VERSION=$(git rev-parse --short HEAD) react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "husky": "1.3.1",
    "lint-staged": "8.1.3",
    "prettier": "1.16.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,json,css}": [
      "prettier --write",
      "git add"
    ]
  }
}
